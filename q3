/* wap to print multiplication table */
#include<stdio.h>
main(){
    int a;
    printf("enter number for which multiplication table has to be printed : ");
    scanf("%d",&a);
    int b;
    printf("enter number till which multiplication table has to be printed : ");
    scanf("%d",&b);
    int c;
    for (int i;i<=b;i++){
        c=a*i;
        printf("%d * %d = %d \n",a,i,c);
        
    }
}
